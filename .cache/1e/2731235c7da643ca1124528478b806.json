false)\n  ajax.send()\n  return JSON.parse(ajax.response)\n}\n\nfunction newsFeed() {\n  const newsFeed = getData(NEWS_URL)\n  store.maxLength = newsFeed.length\n\n  const newsList = []\n\n  newsList.push(\"<ul>\")\n  for (\n    let i = (store.currentPage - 1) * 10;\n    i < store.currentPage * 10;\n    i += 1\n  ) {\n    newsList.push(`\n  <li>\n    <a href=\"#/show/${newsFeed[i].id}\">\n      ${newsFeed[i].title} - (${newsFeed[i].comments_count})\n    </a>\n  </li>\n  `)\n  }\n  newsList.push(\"</ul>\")\n  newsList.push(`\n  <div>\n  <a href=\"#/page/${\n    store.currentPage > 1 ? store.currentPage - 1 : 1\n  }\">이전 페이지</a>\n  <a href=\"#/page/${\n    Math.ceil(store.maxLength / 10) === store.currentPage\n      ? store.currentPage\n      : store.currentPage + 1\n  }\">다음 페이지</a>\n  </div>\n  `)\n\n  container.innerHTML = newsList.join(\"\")\n}\n\nfunction newsDetail() {\n  const id = location.hash.substring(7)\n\n  const newsContent = getData(CONTENT_URL.replace(\"@id\", id))\n\n  container.innerHTML = `\n    <h1>${newsContent.title}</h1>\n\n    <div>\n      <a href=\"#/page/${store.currentPage}\">목록으로</a>\n    </div>\n  `\n}\n\nfunction router() {\n  const routePath = location.hash\n\n  if (routePath === \"\") newsFeed()\n  else if (routePath.indexOf(\"#/page/\") >= 0) {\n    store.currentPage = Number(routePath.substring(7))\n    newsFeed()\n  } else newsDetail()\n}\n\nwindow.addEventListener(\"hashchange\", router)\n\nrouter()\n"},"lineCount":null}},"error":null,"hash":"300c18e28e1442b9dc7e60a06f209b4c","cacheData":{"env":{}}}